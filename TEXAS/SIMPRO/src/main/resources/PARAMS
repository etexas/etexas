      INTEGER           NTS
      PARAMETER       ( NTS    = NTC + 1             )
C-----NVEP1    501 NUMBER OF VEHICLES IN THE SYSTEM AT ONE TIME PLUS 1
      INTEGER           NVEP1
      PARAMETER       ( NVEP1  = NVE + 1             )
C-----POSBIG  5000 POSITION THAT WILL NEVER BE POSSIBLE
      DOUBLE PRECISION  POSBIG
      PARAMETER       ( POSBIG = 4.0D0*POSMAX        )
