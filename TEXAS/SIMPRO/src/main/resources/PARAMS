      INTEGER           VMSMCL
      PARAMETER       ( VMSMCL =         7           )
C-----VMSMCR     8 VMS MESSAGE
C-----             CHANGE LANES TO THE RIGHT
      INTEGER           VMSMCR
      PARAMETER       ( VMSMCR =         8           )
C-----VMSMGO     9 VMS MESSAGE
C-----             FORCED GO
      INTEGER           VMSMGO
      PARAMETER       ( VMSMGO =         9           )
C-----VMSMRR    10 VMS MESSAGE
C-----             FORCED RUN THE RED SIGNAL
      INTEGER           VMSMRR
      PARAMETER       ( VMSMRR =        10           )
