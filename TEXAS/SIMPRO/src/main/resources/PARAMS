      INTEGER           VDIFDF
      PARAMETER       ( VDIFDF =        30           )
C-----VDIFIP    31 VEHICLE FORCED TO USE DIFFERENT INTERSECTION PATH
      INTEGER           VDIFIP
      PARAMETER       ( VDIFIP =        31           )
C-----VDILCL    32 LANE CHANGE NEEDS MORE LENGTH IN FIRST PART OF LANE
      INTEGER           VDILCL
      PARAMETER       ( VDILCL =        32           )
C-----VDIBZI    33 BLOCK ZONE INTRUSION - VEHICLE IN BLOCKED PART OF
C-----             LANE
      INTEGER           VDIBZI
      PARAMETER       ( VDIBZI =        33           )
C-----VDIMCL    34 VEHICLE INVOLVED IN A MAJOR COLLISION
      INTEGER           VDIMCL
      PARAMETER       ( VDIMCL =        34           )
C-----VDICZI    35 VEHICLE INVOLVED IN A CLEAR ZONE INTRUSION
      INTEGER           VDICZI
      PARAMETER       ( VDICZI =        35           )
