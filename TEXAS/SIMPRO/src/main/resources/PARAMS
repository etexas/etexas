      INTEGER           NIS
      PARAMETER       ( NIS    = NIA + 2             )
C-----NLGP1      7 NUMBER OF LEGS PLUS 1
      INTEGER           NLGP1
      PARAMETER       ( NLGP1  = NLG + 1             )
